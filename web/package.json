{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-tailwind && npm run build-remix",
    "build-remix": "remix build",
    "build-tailwind": "tailwindcss -m -i ./styles/main.css -o app/styles/app.css",
    "dev": "concurrently \"npm:dev-*\"",
    "dev-remix": "remix dev",
    "dev-tailwind": "tailwindcss -w -i ./styles/main.css -o app/styles/app.css",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/joy": "^5.0.0-alpha.42",
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.6.8",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@sanity/client": "^3.3.3",
    "@sanity/icons": "^1.3.4",
    "@sanity/ui": "^0.37.19",
    "@stripe/stripe-js": "^1.35.0",
    "cross-env": "^7.0.3",
    "groq": "^2.29.3",
    "node-mailjet": "^5.1.1",
    "prettier": "^2.7.1",
    "react": "^17.x.x",
    "react-dom": "17.x.x",
    "stripe": "^10.8.0",
    "tiny-invariant": "^1.2.0",
    "use-shopping-cart": "^3.1.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@types/react": "^17.x.x",
    "@types/react-dom": "^17.x.x",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
